{
    "manifest_version": 2,
    "name": "A2OJ Enhancer",
    "version": "1.0",
    "description": "Improves experience of a2oj.",
    "icons": {
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.a2oj.com/*"
            ],
            "js": [
                "js/enhance.js"
            ]
        }
    ],
    "browser_action" : {
        "default_icon" : "icons/icon-128.png" ,
        "default_title" : "A2OJ Enhancer"
    },
    "permissions" : [
        "*://codeforces.com/*",
        "storage"
    ]
}